<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <title>User list</title>
</head>
<body>
    <!-- <p><a href="/create">ユーザ追加</a></p> -->
    <table>
        <tr>
            <th>user</th>
            <th>password</th>
            <th>reset password</th>
            <th>delete</th>
        </tr>
        <% users.forEach(function (value) { %>
        <tr>
            <td><%= value.name %></td>
            <td><%= value.email %></td>
            <td><a href="/edit/<%= value.id %>">reset</a></td>
            <td><a href="/delete/<%= value.id %>">delete</a></td>
        </tr>
        <% }); %>
    </table>
</body>
</html>